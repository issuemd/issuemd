{
  "name": "issuemd",
  "version": "0.2.1",
  "description": "issue tracking in hand",
  "main": "issuemd.min.js",
  "scripts": {
    "start": "npm run test && npm run build",
    "test": "jest --coverage",
    "build": "npm run build:parser && webpack",
    "build:parser": "pegjs < src/parser/issuemd-parser.pegjs > issuemd.parser.js",
    "precommit": "pretty-quick --staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/issuemd/issuemd.git"
  },
  "keywords": [
    "distributed",
    "issue",
    "tracking",
    "issuemd"
  ],
  "author": "Billy Moon (billy@issue.md)",
  "contributors": [
    {
      "name": "Luka Siric",
      "email": "luka@issue.md"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/issuemd/issuemd/issues"
  },
  "homepage": "https://github.com/issuemd/issuemd",
  "tonicExampleFilename": ".tonic.js",
  "jest": {
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "pegjs": "^0.9.0",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0",
    "webpack": "^4.16.3",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {
    "blueimp-md5": "^2.10.0",
    "marked": "^0.4.0",
    "mustache": "^2.2.0"
  }
}
